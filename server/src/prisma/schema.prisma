generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  uid         String   @id
  displayName String
  email       String   @unique
  bio         String   @default("")
  avatar      String   @default("")
  createdAt   DateTime @default(now())
}
